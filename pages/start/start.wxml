<!--pages/start/start.wxml-->
<loading wx:if="{{showLoading}}">加载中</loading>
<view class='start'>
<!-- 背景图 -->
<view class="start-image">
  <image src="../../images/start.jpg"></image>
</view>
</view>

<!-- 页面按钮 -->
<view class="start-anniu">
  <view class="start-title">{{datas.system_name}}</view>
  <view class="start-nr">{{datas.system_desc}}</view>
  <button class="start-button" open-type="getUserInfo" bindGetUserInfo='bindGetUserInfo'>开启新生活</button>
</view>
