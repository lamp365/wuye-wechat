page{
	background: #F0F0F0;
}
.notice{
	width: 100%;
	height: auto;
}
.noticelist{
	width: 100%;
	height: auto;
	background: #fff;
	margin-bottom: 30rpx;
}
.list-xq{
	padding-bottom: 35rpx;
}
.list-xq view:first-child{
	font-size: 32rpx;
	padding: 30rpx 0 27rpx 0;
	color: #000;
}
.list-xq view:nth-child(2){
	font-size: 26rpx;
	line-height: 37rpx;
	color: #999;
	margin-bottom: 21rpx;
}
.list-xq view:last-child text{
	font-size: 24rpx;
	color: #ccc;
	margin-right: 46rpx;
}


/* 新闻页面 */
.news-item {
  width: 100%;
  height: auto;
}

.news-i-one {
  width: 702rpx;
  height: 150rpx;
  padding: 20rpx 24rpx;
  margin-bottom: 10rpx;
  display: flex;
  justify-content: space-between;
  background: #fff;
}

.news-i-one>view:first-child {
  width: 240rpx;
  height: 150rpx;
  margin-right: 25rpx;
}

.news-i-one>view:first-child>image {
  width: 100%;
  height: 100%;
}

.news-one-right {
  flex: 1;
  height: auto;
}

.news-one-right>view:first-child {
  width: 100%;
  height: 64rpx;
  line-height: 32rpx;
  font-size: 28rpx;
  color: #333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}

.news-one-right>view:nth-child(2) {
  font-size: 24rpx;
  color: #999;
  margin-top: 15rpx;
}

.news-one-right>view:last-child {
  font-size: 24rpx;
  color: #999;
  display: flex;
  justify-content: space-between;
}

.news-one-right>view:last-child>text {
  color: #f34a51;
}


/*空页面*/
.cart-kong{
  width: 100%;
  height: 100vh;
}
.cart-kong>image{
  width: 300rpx;
  height: 300rpx;
  margin: 300rpx 225rpx 30rpx 225rpx;
}
.cart-kong>view:nth-child(2){
  font-size: 34rpx;
  color: #333;
  text-align: center;
}
.cart-kong>view:last-child{
  font-size: 30rpx;
  color: #666;
  margin-top: 15rpx;
  text-align: center;
}
.index-qb{
  width: 100%;
  height: auto;
  text-align: center;
  font-size: 24rpx;
  color: #666;
  margin: 20rpx 0;
}